coordenadas = []

geolocator = nominatim(user_agent="escolas_geocoding")

for escola in escolas:
    location = geolocator.geocode(escola + ", Bahia, Brasil")
    if location:
        coordenadas.append((escola, location.latitude, location.logitude))

for escola, lat, lng in coordenadas:
    print(f"{escola}: Latitude {lat}, Longitude {lng}")
